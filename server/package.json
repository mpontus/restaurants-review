{
  "name": "restaurants-review-server",
  "version": "1.0.0",
  "description": "API server for restaurants review app",
  "main": "index.js",
  "author": "Michael Pontus",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "nodemon",
    "migration:generate": "ts-node -r tsconfig-paths/register node_modules/.bin/typeorm migration:generate",
    "migration:run": "ts-node -r tsconfig-paths/register node_modules/.bin/typeorm migration:run",
    "migrate": "typeorm migration:run",
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "test:e2e": "jest --runInBand --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "@nestjs/cqrs": "^5.1.1",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/testing": "^5.1.0",
    "@nestjs/typeorm": "^5.2.2",
    "bcrypt": "^3.0.1",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "cli-ux": "^4.9.3",
    "cors": "^2.8.4",
    "glob": "^7.1.3",
    "io-ts": "^1.4.0",
    "ioredis": "^4.2.0",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "nestjs-config": "^1.2.5",
    "pg": "^7.4.3",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "typeorm": "^0.2.7",
    "typescript": "^3.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.4",
    "@types/glob": "^7.1.1",
    "@types/ioredis": "^4.0.4",
    "@types/jest": "^23.3.1",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "@types/uuid": "^3.4.4",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "tslint": "5.11.0",
    "tslint-clean-code": "^0.2.7",
    "tslint-config-prettier": "^1.15.0",
    "typemoq": "^2.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
